FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 0.07;

boundaryField
{
    ground
    {
        type            epsilonWallFunction;
        value           $internalField;
    }
    outlet
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }
    inlet
    {
        type            fixedValue;
        value           $internalField;
    }
    blade
    {
        type            epsilonWallFunction;
        value           $internalField;
    }
    hub
    {
        type            epsilonWallFunction;
        value           $internalField;
    }
    cabin
    {
        type            epsilonWallFunction;
        value           $internalField;
    }
    tower
    {
        type            epsilonWallFunction;
        value           $internalField;
    }
    AMI1
    {
        type            cyclicAMI;
        value           $internalField;
    }
    AMI2
    {
        type            cyclicAMI;
        value           $internalField;
    }
    farField
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }
}


// ************************************************************************* //
